{
  "name": "react-p5-wrapper",
  "version": "3.0.0",
  "description": "A wrapper component that allows you to utilise P5 sketches within React apps.",
  "homepage": "https://github.com/jamesrweb/react-p5-wrapper",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "npm run clean && rollup -c",
    "build:demo": "npm run clean && webpack --config webpack.config.demo.js --mode production",
    "clean": "rimraf dist",
    "format": "prettier --config .prettierrc --write '**/*.{js,jsx,ts,tsx,md,json}'",
    "lint": "eslint example/ src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "publish": "npm run build-demo && gh-pages -d dist && npm run build && npm publish",
    "start": "webpack serve --config webpack.config.demo.js --env development --mode development",
    "test": "jest"
  },
  "keywords": [
    "react",
    "react-component",
    "p5",
    "processing"
  ],
  "contributors": [
    {
      "name": "James Robb",
      "url": "https://github.com/jamesrweb"
    },
    {
      "name": "Eugene Dyko",
      "url": "https://github.com/yevdyko"
    },
    {
      "name": "Andreas Wolf",
      "email": "info@wolfandreas.com"
    },
    {
      "name": "Ivan Malyugin"
    },
    {
      "name": "Benjamin Saphier",
      "url": "https://github.com/bsaphier"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jamesrweb/react-p5-wrapper.git"
  },
  "bugs": {
    "url": "https://github.com/jamesrweb/react-p5-wrapper/issues"
  },
  "dependencies": {
    "deep-equal": "^1.1.1",
    "p5": "^1.3.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@types/deep-equal": "^1.0.1",
    "@types/jest": "^26.0.23",
    "@types/p5": "^1.3.0",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-loader": "^8.2.2",
    "canvas": "^2.8.0",
    "css-loader": "^5.2.6",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.0.5",
    "prettier": "^2.3.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.4",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "prettier": {
    "trailingComma": "none"
  },
  "eslintignore": [
    "dist",
    "node_modules"
  ]
}
